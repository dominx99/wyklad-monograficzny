<table class="table table-stripped">
    <thead>
        <tr>
            {% for head in headers %}
                <th>{{ head }}</th>
            {% endfor %}
        </tr>
    </thead>
    <tbody>
        {% for row in table %}
            <tr>
                {% for cell in row %}
                    <td>{{ cell }}</td>
                {% endfor %}
            </tr>
        {% endfor %}
    </tbody>
</table>
